    <script>
        // Funktion, um den benutzerdefinierten Cursor beim Rechtsklick anzuzeigen
        const showTrollFaceCursorOnRightClick = () => {
          // Wenn der Benutzer mit der rechten Maustaste klickt
          $(document).on('contextmenu', (e) => {
            e.preventDefault(); // Verhindert das Standard-Kontextmenü
    
            // Cursor auf 'trollface.jpg' setzen
            document.body.style.cursor = 'url("./assets/others/trollface.jpg") 16 16, auto';
          });
    
          // Wenn der Benutzer irgendwo anders klickt, zurück zum Standardcursor
          $(document).on('click', () => {
            document.body.style.cursor = 'default'; // Setzt den Standardcursor
          });
        };
    
        // Initialisierung der Funktionen
        $(document).ready(() => {
          // Aktiviert die Rechtsklick-Cursor-Funktion
          showTrollFaceCursorOnRightClick();
        });
      </script>

    <script type="text/javascript" src="assets/js/script.js"></script>   

